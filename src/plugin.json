{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Extensions DevTools",
  "id": "grafana-extensionsdependencygraph-app",
  "info": {
    "keywords": ["app", "extensions", "devtools", "developer-tools", "plugin-development"],
    "description": "Developer tools for plugin authors working with Grafana's plugin extensions system. Visualize and explore extension points, extensions, and plugin relationships in your development workflow.",
    "author": {
      "name": "Grafana"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Added Links Mode",
        "path": "img/added-links.jpg"
      },
      {
        "name": "Added Components Mode",
        "path": "img/added-components.jpg"
      },
      {
        "name": "Added Functions Mode",
        "path": "img/added-functions.jpg"
      },
      {
        "name": "Exposed Components Mode",
        "path": "img/exposed-components.jpg"
      },
      {
        "name": "Extension Points Mode",
        "path": "img/extension-points.jpg"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Dependency Graph",
      "path": "/a/%PLUGIN_ID%/dependency-graph",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "panel",
      "name": "Dependency Graph Panel",
      "path": "dependency-graph/dependency-graph-panel"
    }
  ],
  "extensions": {
    "addedComponents": [
      {
        "targets": ["grafana/extension-sidebar/v0-alpha"],
        "description": "Opens Plugin Extensions DevTools",
        "title": "Plugin Extensions DevTools"
      }
    ],
    "addedLinks": [
      {
        "targets": ["grafana/extension-sidebar/v0-alpha"],
        "description": "Opens Plugin Extensions DevTools",
        "title": "Plugin Extensions DevTools"
      }
    ]
  },
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
  }
}
